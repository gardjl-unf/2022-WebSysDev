@model IEnumerable<string>
    <a class="btn btn-outline-dark btn-block m-2" asp-action="List" asp-controller="Vehicle" asp-route-category="">Home</a>
    @foreach (string category in Model)
    {
        <a class="btn btn-outline-dark btn-block m-2" asp-action="List" asp-controller="Vehicle" asp-route-category="@category" asp-route-page="1">@category</a>
    }