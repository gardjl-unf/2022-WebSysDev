@model ProductsListViewModel

@{
    Layout = "_Layout2";
}

@foreach (var p in Model.Products)
{
    @Html.Partial("ProductSummary2", p)
}
<div page-model="@Model.PagingInfo" page-action="List1" page-classes-enabled="true" page-class="btn" 
    page-class-normal="btn-default" page-class-selected="btn-primary" page-url-category="@Model.CurrentCategory" class="btn-group float-right">
</div>
